{
    "manifest_version": 2,
    "name": "Tumblr Tweaks",
    "description": "This Google Chrome extension automatically extends functionality on a Tumblr site.",
    "version": "1.2.3",
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches": ["http://*/*"],
            "js": ["jquery.min.js", "tumblr-tweaks.js"]
        }
    ],
    "web_accessible_resources": ["jquery-1.10.2.min.map"]
}